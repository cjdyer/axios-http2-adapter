{
  "name": "axios-http2-adapter",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "author": "",
  "scripts": {
    "test": "npm run test:eslint && npm run test:mocha && npm run test:exports",
    "test:mocha": "mocha test/unit/**/*.js --timeout 30000 --exit",
    "test:eslint": "eslint lib/**/*.js",
    "test:exports": "mocha test/module/test.js --timeout 30000 --exit",
    "build": "gulp clear && cross-env NODE_ENV=production rollup -c -m",
    "fix": "eslint --fix lib/**/*.js",
    "prepare": "husky install && npm run prepare:hooks",
    "prepare:hooks": "npx husky set .husky/commit-msg \"npx commitlint --edit $1\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "cross-env": "^7.0.3",
    "eslint": "~8.56",
    "express": "~4.21",
    "fs-extra": "^11.3.0",
    "gulp": "^5.0.0",
    "husky": "~8.0",
    "lint-staged": "~15.2",
    "mocha": "^11.5.0",
    "rollup": "^2.79.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "follow-redirects": "~1.15",
    "http2-wrapper": "~2.2"
  },
  "peerDependencies": {
    "axios": "^1.9"
  },
  "exports": {
    ".": {
      "browser": {
        "require": "./dist/browser/axios-http2-adapter.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/node/axios-http2-adapter.cjs",
        "default": "./index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        2,
        "always",
        130
      ]
    },
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
